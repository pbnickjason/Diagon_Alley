<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>


<div class=".container">    
    <div class="row">
        <% @projects.each do |project| %>
            <div class="col-md-6">
                <h2><%= link_to "#{project.title}", project %></h2> <br>
                <h4><%= link_to 'Edit', edit_project_path(project) %></h4>
                <h4><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></h4>
            </div>
        <% end %>
    </div>
</div>

<button type="button" class="btn btn-primary btn-lg btn-block"><%= link_to 'New Project', new_project_path %></button>
